#include <stdio.h>

int main() {
    int a = 10, b = 5, c;
    int *ptr = &a;

    // arithmetic operation
    
    printf("Arithmetic Operator:\n");
    printf("a + b = %d\n", a + b);
    printf("a - b = %d\n", a - b);
    printf("a * b = %d\n", a * b);
    printf("a / b = %d\n", a / b);
    printf("a %% b = %d\n\n", a % b); 

    return 0;
}
